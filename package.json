{
  "name": "morphine",
  "version": "1.0.0",
  "description": "A simple boilerplate for React WebApps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidiego/morphine.git"
  },
  "scripts": {
    "start": "webpack-dev-server --progress --colors",
    "build": "webpack --config webpack.config.js",
    "test": "karma start --single-run --no-auto-watch karma.config.js",
    "test:watch": "nkarma start --auto-watch --no-single-run karma.config.js",
    "lint": "eslint ./src",
    "coveralls": "cat ./coverage/report-lcov/lcov.info | coveralls"
  },
  "keywords": [
    "react",
    "es6",
    "sass",
    "webpack",
    "javascript",
    "universal"
  ],
  "author": "Guilherme Diego <guidiego.expgames@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guidiego/morphine/issues"
  },
  "homepage": "https://github.com/guidiego/morphine#readme",
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.2",
    "coveralls": "^2.11.14",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-node": "^0.2.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "file-loader": "^0.9.0",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
