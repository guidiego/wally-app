{
  "name": "morphine",
  "version": "1.0.0",
  "description": "A simple boilerplate for React WebApps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidiego/morphine.git"
  },
  "scripts": {
    "dev": "next ./src",
    "build": "next build ./src",
    "start": "next start ./src",
    "test": "jest --config jest.config.json --watch",
    "test:cov": "jest --config jest.config.json --coverage",
    "docker_build": "docker build -t morphine $PWD",
    "docker_run": "docker run -it -p 3000:3000 morphine",
    "lint": "eslint ./src",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "react",
    "es6",
    "sass",
    "webpack",
    "javascript",
    "universal"
  ],
  "author": "Guilherme Diego <guidiego.expgames@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guidiego/morphine/issues"
  },
  "homepage": "https://github.com/guidiego/morphine#readme",
  "dependencies": {
    "next": "^1.2.3",
    "react": "^15",
    "react-bootstrap": "^0.30.7",
    "react-redux": "^5.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-airbnb": "^2.1.1",
    "coveralls": "^2.11.15",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "isolate-redux-env": "^1.0.0",
    "jest": "^18.0.0",
    "jest-enzyme": "^2.0.0",
    "react-addons-test-utils": "^15.4.1"
  }
}
